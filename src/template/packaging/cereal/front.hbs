<svg x="50%" y="{{randomNumber 10 50}}%" width="100%" height="100%" overflow="visible">
    <svg width="100%" height="100%" transform="rotate({{randomNumber -45 45}})" overflow="visible">
        <text text-anchor="middle" font-size="{{randomNumber 0.3 0.6}}in">{{product.name}}</text>
        <text y="0.2in" dominant-baseline "hanging" text-anchor="middle" font-size="0.2in">{{product.slogan}}</text>
    </svg>
</svg>