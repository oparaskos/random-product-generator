    <rect x="0" y="0" width="100%" height="100%" stroke="red"  fill="url(#background)"></rect>
    {{{sides.right.body}}}
</svg>
<svg x="{{depth}}in" y="{{depth}}in" width="{{width}}in" height="{{height}}in">
    <rect x="0" y="0" width="100%" height="100%" stroke="red"  fill="url(#background)"></rect>
    {{{sides.front.body}}}
</svg>
<svg x="{{sum depth width depth}}in" y="{{depth}}in" width="{{width}}in" height="{{height}}in">
    <rect x="0" y="0" width="100%" height="100%" stroke="red"  fill="url(#background)"></rect>
    {{{sides.back.body}}}
</svg>